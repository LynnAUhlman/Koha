SELECT
  r.reserve_id,
  p.cardnumber,
  r.biblionumber,
  r.timestamp AS "Date Placed",
  r.priority,
  r.branchcode AS "Pickup Library"
FROM
  reserves r
LEFT JOIN borrowers p ON r.borrowernumber = p.borrowernumber
WHERE
  p.cardnumber = [insertpatronbarcode]
  AND r.cancellationdate IS NULL
  AND r.found IS NULL
ORDER BY r.timestamp
